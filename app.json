{
  "expo": {
    "name": "1on1 aka Bonfire",
    "slug": "1on1 aka Bonfire",
    "version": "1.0.0",
    "scheme": "bonfire",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "com.gimbalabs.bonfire",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.gimbalabs.bonfire",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "ts", "tsx", "svg"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
